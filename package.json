{
  "name": "sunshile-cli-dev-server",
  "version": "1.0.0",
  "description": "shushil-cli-dev 服务端",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-sunshile-cli-dev-server",
    "stop": "egg-scripts stop --title=egg-server-sunshile-cli-dev-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.13",
    "@types/supertest": "^2.0.11",
    "egg": "^2.29.4",
    "egg-scripts": "^2.14.0",
    "eslint": "^7.32.0",
    "eslint-config-egg": "^9.0.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "autod": "^3.1.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.16.4",
    "egg-ci": "^1.18.0",
    "egg-mock": "^3.26.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "",
  "license": "MIT"
}
